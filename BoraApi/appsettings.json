{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "SecurityKey": "{securityKey}"
  },
  "ConnectionStrings": {
    "BoraRepository_usingInmemory": "Server=tcp:{dbserver}.database.windows.net,1433;Initial Catalog={database};Persist Security Info=False;User ID={user};Password={password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "GoogleCalendar": {
    "ApplicationName": "Bora",
    "ClientId": "{clientId}",
    "ClientSecret": "{clientSecrect}",
    "TokenFolder": "token.json"
  }
}
